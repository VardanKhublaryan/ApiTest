<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="ListenerExecution"  parallel = "methods" thread-count="11" data-provider-thread-count="6">
    <listeners>
        <listener class-name="util.MyListener"/>
    </listeners>

    <test name="ExecutingListeners">
        <classes>
            <class name="CreateAndUpdate"/>
            <class name="DataFields"/>
            <class name="Login"/>
            <class name="UserRegister"/>
        </classes>
    </test>
</suite>